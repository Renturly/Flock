<script>
    const doggingFistfuls = async () => {
	const portfolio = await fetch("https://www.accesstheflock.io/matchTable/DoggingFistfuls");
    
	const portfolioJSON = await portfolio.json();
	
	console.log(portfolioJSON.portfolio_id);
	document.getElementById('portfolio').innerHTML = portfolioJSON.portfolio_id;
	

}

	doggingFistfuls();


//.then() chained to fetch callback functions for success and error
fetch("https://www.accesstheflock.io/matchTable/DoggingFistfuls")
.then(
		response => {
			response.text().then(function (text) {
  			console.log(JSON.parse(text)); 
});
		},
		rejection => {
		console.error(rejection.message);
		});

</script>
