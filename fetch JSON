<script>
    const doggingFistfuls = async () => {
	const portfolio = await fetch("https://www.accesstheflock.io/matchTable/DoggingFistfuls");
    
	const portfolioJSON = await portfolio.json();
	
	console.log(portfolioJSON.portfolio_id);
	document.getElementById('portfolio').innerHTML = portfolioJSON.portfolio_id;
	

}

	doggingFistfuls();

</script>
